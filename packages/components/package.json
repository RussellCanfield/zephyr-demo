{
  "name": "@acme/components",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles": {
      "import": "./dist/esm/index.css",
      "require": "./dist/cjs/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "tslib": "^2.3.0",
    "postcss-alias": "2.0.0",
    "@module-federation/rsbuild-plugin": "0.6.13",
    "@rsbuild/plugin-react": "1.0.5",
    "@rsbuild/core": "1.0.18",
    "@acme/utils": "workspace:*",
    "react-router-dom": "6.27.0",
    "jotai": "2.10.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "private": true,
  "type": "module"
}
